{
  "name": "earthquake-geoserve-ui",
  "version": "0.0.4",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "audit": "auditjs > /dev/null; count=$?; if [ ${count} -lt 20 ]; then exit 0; else exit ${count}; fi",
    "start": "npm run build && ng serve -o",
    "build": "ng build --prod hazdev-ng-geoserve-output",
    "buildApp": "ng build --prod --delete-output-path false --base-href /geoserve/",
    "test": "ng test --source-map=false hazdev-ng-geoserve-output",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "publish": "npm run build && cd dist/hazdev-ng-geoserve-output && npm publish"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "6.1.8",
    "@angular/cdk": "^6.2.1",
    "@angular/common": "6.1.8",
    "@angular/compiler": "6.1.8",
    "@angular/core": "6.1.8",
    "@angular/forms": "6.1.8",
    "@angular/http": "6.1.8",
    "@angular/material": "^6.2.1",
    "@angular/platform-browser": "6.1.8",
    "@angular/platform-browser-dynamic": "6.1.8",
    "@angular/router": "6.1.8",
    "auditjs": "^3.0.10",
    "core-js": "^2.5.3",
    "hazdev-ng-location-view": "0.0.4",
    "leaflet": "^1.2.0",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.3",
